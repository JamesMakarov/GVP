<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.controladores.criaritem.escolhaenums.roupacomumenums.CalcadoEnums">
    <center>
        <VBox fx:id="vBoxTipos" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Tipos">
                            <font>
                                <Font name="System Bold" size="17.0" />
                            </font>
                        </Text>
                    </children>
                </HBox>
                <ListView fx:id="enumCalcado" minHeight="100.0">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin></ListView>
                <VBox prefHeight="200.0" prefWidth="100.0" spacing="20.0">
                    <children>
                        <TextField fx:id="nome" promptText="Nome">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </padding>
                        </TextField>
                        <TextField fx:id="cor" promptText="Cor">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </padding>
                        </TextField>
                        <TextField fx:id="tamanho" promptText="Tamanho">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </padding>
                        </TextField>
                        <TextField fx:id="marca" promptText="Marca">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </padding>
                        </TextField>
                        <TextField fx:id="estado" promptText="Estado">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </padding>
                        </TextField>
                    </children>
                    <VBox.margin>
                        <Insets top="15.0" />
                    </VBox.margin>
                </VBox>
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" />
            </padding>
        </VBox>
    </center>
    <left>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
    </left>
    <right>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
    </right>
    <top>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Preencha os campos a seguir" BorderPane.alignment="CENTER">
            <font>
                <Font name="System Bold" size="28.0" />
            </font>
        </Text>
    </top>
    <bottom>
        <Button fx:id="confirmar" mnemonicParsing="false" prefHeight="34.0" prefWidth="103.0" style="-fx-background-color: #00ffff;" text="Confirmar" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="15.0" />
            </BorderPane.margin>
        </Button>
    </bottom>
</BorderPane>
