<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane fitToWidth="true" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.controladores.ListaGRControlador">
    <content>
        <VBox prefWidth="584.0" spacing="10.0">
            <children>
                <ToolBar prefHeight="40.0" prefWidth="591.0">
                    <items>
                        <Button fx:id="btnAdicionar" mnemonicParsing="false" style="-fx-background-color: transparent;" text="Adicionar guarda-roupa">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font></Button>
                  <Button fx:id="btnDeletar" mnemonicParsing="false" style="-fx-background-color: transparent;" text="Deletar guarda-roupa">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                    </items>
                </ToolBar>
            <ListView fx:id="listaDeGuardaRoupas" prefHeight="347.0" prefWidth="598.0" />
            </children>
        </VBox>
    </content>
</ScrollPane>
